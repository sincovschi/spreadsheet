{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "spreadsheet",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "scss", "linter": "eslint", "babel": true },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "spreadsheet": {
      "root": "apps/spreadsheet",
      "sourceRoot": "apps/spreadsheet/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/spreadsheet",
            "index": "apps/spreadsheet/src/index.html",
            "main": "apps/spreadsheet/src/main.tsx",
            "polyfills": "apps/spreadsheet/src/polyfills.ts",
            "tsConfig": "apps/spreadsheet/tsconfig.app.json",
            "assets": [
              "apps/spreadsheet/src/favicon.ico",
              "apps/spreadsheet/src/assets"
            ],
            "styles": ["apps/spreadsheet/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/spreadsheet/src/environments/environment.ts",
                  "with": "apps/spreadsheet/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": { "buildTarget": "spreadsheet:build" },
          "configurations": {
            "production": { "buildTarget": "spreadsheet:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/spreadsheet/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/spreadsheet"],
          "options": {
            "jestConfig": "apps/spreadsheet/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "spreadsheet-e2e": {
      "root": "apps/spreadsheet-e2e",
      "sourceRoot": "apps/spreadsheet-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/spreadsheet-e2e/cypress.json",
            "tsConfig": "apps/spreadsheet-e2e/tsconfig.e2e.json",
            "devServerTarget": "spreadsheet:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "spreadsheet:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/spreadsheet-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "common-components": {
      "root": "libs/common-components",
      "sourceRoot": "libs/common-components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/common-components/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common-components"],
          "options": {
            "jestConfig": "libs/common-components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
